{
  "manifest_version": 3,
  "name": "Goblin Snack Report",
  "version": "1.0.0",
  "description": "Feeds hungry gobbos.",
  "permissions": [],
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "goblin.png"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ]
    }
  ]
}
